<%= form_for @studysession, :url => {action: "create"}, :html => { :class => 'form-horizontal' } do |f| %>
  
  <div class="control-group">
    <%= f.label :title,:class => 'control-label'%><br />
    <div class ="controls">    
      <%= text_field_tag :title %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :description,:class => 'control-label' %><br />
    <div class ="controls">    
      <%= text_field_tag :description %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :location,:class => 'control-label' %><br />
    <div class ="controls">    
      <%= text_field_tag :location %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :date,:class => 'control-label' %><br />
    <div class ="controls">    
      <%= text_field_tag :date %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :time,:class => 'control-label' %><br />
    <div class ="controls">    
      <%= text_field_tag :time %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :category,:class => 'control-label' %><br />
    <div class ="controls">    
      <%= text_field_tag :category %>
    </div>
  </div>
Top Recommendations Time Maker<br>
<% @course.getTimingRecommendation.each do |key,value| %>
  <%= key +"--> " + "Best Time -->" + value[0] + " Number of Classmates ->> " + value[1].to_s%>
  <br>
<%end%>

<%= f.submit nil, :class => 'btn btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
              dashboard_path(current_user), :class => 'btn' %>

<% end %>
